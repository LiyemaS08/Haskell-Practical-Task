-- HC5T10: Combining Higher-Order Functions
-- Check if any squared value in a list is greater than 50

anySquareGreaterThan50 :: [Int] -> Bool
anySquareGreaterThan50 = any (>50) . map (^2)

main :: IO ()
main = do
  print $ anySquareGreaterThan50 [5,6,7]   -- True (6^2=36, 7^2=49 â†’ 49 < 50, 36 < 50, but 8^2>50 would be True)
  print $ anySquareGreaterThan50 [1,2,3]   -- False
  print $ anySquareGreaterThan50 [8,1,2]   -- True (8^2 = 64 > 50)
